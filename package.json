  "scripts": {
    "migrate": "npx nx@latest migrate latest && rm -rf node_modules package-lock.json && npm nx@latest migrate --run-migrations --create-commits",
    "update-deps:dry-run": "npm-check-updates",
    "update-deps": "npm-check-updates -u && npm install",
    "generate-icons:pangu": "npx pwa-asset-generator apps/pangu/src/assets/icons/logo.svg apps/pangu/src/assets/icons/ --manifest apps/pangu/src/manifest.webmanifest --index apps/pangu/src/index.html --path-override 'assets/icons' --opaque false --padding 0 --type png --mstile && npx nx format pangu",
    "generate-icons:vatti": "npx pwa-asset-generator apps/vatti/public/icons/logo.svg apps/vatti/public/icons/ --manifest apps/vatti/public/manifest.webmanifest --index apps/vatti/src/index.html --path-override 'icons' --opaque false --padding 0 --type png --mstile && npx nx format vatti",
